<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <parent>
        <artifactId>pandama-user</artifactId>
        <groupId>com.pandama.top</groupId>
        <version>1.0.0</version>
    </parent>
    <modelVersion>4.0.0</modelVersion>

    <artifactId>pandama-user-service</artifactId>

    <dependencies>
        <!--web模块-->
        <dependency>
            <groupId>com.pandama.top</groupId>
            <artifactId>pandama-starter-web</artifactId>
            <version>1.0.0</version>
        </dependency>
        <!--user模块api-->
        <dependency>
            <groupId>com.pandama.top</groupId>
            <artifactId>pandama-user-open</artifactId>
            <version>1.0.0</version>
        </dependency>
        <!--auth模块api-->
        <dependency>
            <groupId>com.pandama.top</groupId>
            <artifactId>pandama-auth-open</artifactId>
            <version>1.0.0</version>
        </dependency>
        <!--database模块-->
        <dependency>
            <groupId>com.pandama.top</groupId>
            <artifactId>pandama-database</artifactId>
            <version>1.0.0</version>
        </dependency>
        <!--日志模块-->
        <dependency>
            <groupId>com.pandama.top</groupId>
            <artifactId>pandama-logrecord</artifactId>
            <version>1.0.0</version>
        </dependency>
        <!--java反射框架-->
        <dependency>
            <groupId>org.reflections</groupId>
            <artifactId>reflections</artifactId>
            <version>0.9.11</version>
        </dependency>
        <!--集成rocketmq-->
<!--        <dependency>-->
<!--            <groupId>com.pandama.top</groupId>-->
<!--            <artifactId>pandama-rocketmq</artifactId>-->
<!--            <version>1.0.0</version>-->
<!--        </dependency>-->
        <!--集成websocket-->
        <dependency>
            <groupId>com.pandama.top</groupId>
            <artifactId>pandama-websocket</artifactId>
            <version>1.0.0</version>
        </dependency>
    </dependencies>

    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <configuration>
                    <source>1.8</source>
                    <target>1.8</target>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.springframework.boot</groupId>
                <artifactId>spring-boot-maven-plugin</artifactId>
                <version>${spring-boot.version}</version>
                <configuration>
                    <excludes>
                        <exclude>
                            <groupId>org.projectlombok</groupId>
                            <artifactId>lombok</artifactId>
                        </exclude>
                    </excludes>
                </configuration>
                <executions>
                    <execution>
                        <goals>
                            <goal>repackage</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
        </plugins>
    </build>

</project>